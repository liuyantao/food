<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.dllo.food">

    <uses-permission android:name="android.permission.INTERNET"/>

    <action android:name="androd.media.action.IMAGE_CAPTURE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.RegisterActivity"/>
        <activity android:name=".activity.SetActivity"/>
        <activity android:name=".activity.homepageactivity.HomePageEvaluationNextActivity"/>
        <activity android:name=".activity.homepageactivity.HomePageHomeNextActivity"/>

        <category android:name="android.intent.category.DEFAULT"/>

        <activity android:name=".activity.LibraryNextActivity">
        </activity>
        <activity android:name=".activity.LibrarySearchActivity">
        </activity>
    </application>

</manifest>